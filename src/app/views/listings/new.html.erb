<div class="container">
  <div class="back-button">
    <%= link_to listings_path do %>
      <div class="btn btn-light text-muted text-center">
        <%= fa_icon "chevron-left" %>
        <span>Back to listings</span>
      </div>
    <%end%>
  </div>
  <div class="py-5 create-listing">
    <h2 class="text-center">Create a listing</h2>
    <%= render "form", listing: @listing%>
  </div>
</div>
